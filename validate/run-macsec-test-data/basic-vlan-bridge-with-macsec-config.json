          "enable-restricted-vlan-registration": false,
          "enable-vid-translation-table": false,
          "enable-egress-vid-translation-table": false
        },
        "ieee802-dot1x:pae": {
          "logon-process": {
            "logon": false
          }
        },
        "ieee802-dot1ae:secy": {
          "verification": {
            "validate-frames": "strict",
            "replay-protect": true,
            "replay-window": 0
          },
          "generation": {
            "protect-frames": true,
            "always-include-sci": false,
            "use-es": false,
            "use-scb": false
          },
          "controlled-interface": {
            "admin-point-to-point-mac": "auto"
          },
          "uncontrolled-interface": {
            "admin-point-to-point-mac": "auto"
          }
        },
        "ieee802-dot1ae-pry:pry": {
          "mac-privacy-enabled": true,
          "use-pae-dest-address": true
        }
      },
      {
        "name": "eth1",
        "type": "iana-if-type:ethernetCsmacd",
