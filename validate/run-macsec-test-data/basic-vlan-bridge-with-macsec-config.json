        "component": [
          {
            "name": "cv1",
            "id": 1,
            "type": "c-vlan-component",
            "traffic-class-enabled": true,
            "filtering-database": {
              "aging-time": 300
            }
          },
          {
            "name": "cv2",
            "id": 2,
            "type": "c-vlan-component",
            "traffic-class-enabled": true,
            "filtering-database": {
              "aging-time": 300
            }
          }
        ]
      }
    ]
  },
  "ietf-interfaces:interfaces": {
    "interface": [
      {
        "name": "eth0",
        "type": "iana-if-type:bridge",
        "enabled": true,
        "ieee802-dot1q-bridge:bridge-port": {
          "bridge-name": "bridge1",
          "component-name": "cv1",
          "port-type": "c-vlan-bridge-port",
          "pvid": 1,
          "default-priority": 0,
          "priority-regeneration": {
            "priority0": 0,
            "priority1": 1,
            "priority2": 2,
            "priority3": 3,
            "priority4": 4,
            "priority5": 5,
            "priority6": 6,
            "priority7": 7
          },
          "pcp-selection": "8P0D",
          "use-dei": false,
          "drop-encoding": false,
          "service-access-priority-selection": false,
          "service-access-priority": {
            "priority0": 0,
            "priority1": 1,
            "priority2": 2,
            "priority3": 3,
            "priority4": 4,
            "priority5": 5,
            "priority6": 6,
            "priority7": 7
