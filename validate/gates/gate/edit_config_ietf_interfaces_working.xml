<?xml version="1.0" encoding="utf-8"?>
<if:interfaces xmlns:if="urn:ietf:params:xml:ns:yang:ietf-interfaces">
  <if:interface>
    <if:name>eth</if:name>
    <if:type xmlns:ianaift="urn:ietf:params:xml:ns:yang:iana-if-type">ianaift:ethernetCsmacd</if:type>
    <dot1q:bridge-port xmlns:dot1q="urn:ieee:std:802.1Q:yang:ieee802-dot1q-bridge">
      <bridge-sched:gate-parameter-table
         xmlns:bridge-sched="urn:ieee:std:802.1Q:yang:ieee802-dot1q-bridge-sched"
         xmlns:sched="urn:ieee:std:802.1Q:yang:ieee802-dot1q-sched">
        <bridge-sched:admin-control-list>
          <bridge-sched:gate-control-entry>
            <bridge-sched:index>1</bridge-sched:index>
            <bridge-sched:operation-name>sched:set-gate-states</bridge-sched:operation-name>
            <bridge-sched:time-interval-value>1000</bridge-sched:time-interval-value>
            <bridge-sched:gate-states-value>255</bridge-sched:gate-states-value>
          </bridge-sched:gate-control-entry>
        </bridge-sched:admin-control-list>
        <bridge-sched:admin-cycle-time>
          <bridge-sched:numerator>1</bridge-sched:numerator>
          <bridge-sched:denominator>1</bridge-sched:denominator>
        </bridge-sched:admin-cycle-time>
        <bridge-sched:supported-list-max>256</bridge-sched:supported-list-max>
        <bridge-sched:supported-cycle-max>
          <bridge-sched:numerator>1</bridge-sched:numerator>
          <bridge-sched:denominator>1</bridge-sched:denominator>
        </bridge-sched:supported-cycle-max>
        <bridge-sched:supported-interval-max>10000</bridge-sched:supported-interval-max>
      </bridge-sched:gate-parameter-table>
    </dot1q:bridge-port>
  </if:interface>
</if:interfaces>
