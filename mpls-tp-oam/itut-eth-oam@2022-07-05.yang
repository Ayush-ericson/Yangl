module itut-eth-oam {
    namespace "urn:itu:t:rec:g.8052.1:yang:itut-eth-oam";
    prefix itut-eth-oam;
    import ieee802-dot1q-cfm {
        prefix ieee802-dot1q-cfm;
    }
    import ieee802-dot1q-cfm-types {
        prefix ieee802-dot1q-cfm-types;
    }
    import ieee802-dot1q-types {
        prefix ieee802-dot1q-types;
    }
    import ieee802-types {
        prefix ieee802-types;
    }
    import ieee802-dot1q-cfm {
       prefix dot1q-cfm;
    }
    organization "ITU-T Study Group 15 (SG15) Question 14 (Q14/15)";
    contact "
        Q14/15 Web: <https://www.itu.int/en/ITU-T/studygroups/
                     2017-2020/15/Pages/q14.aspx>
        
        Q14/15 List: <t17sg15q14@lists.itu.int>
        
        
        Editor:   Kam LAM
                  <mailto:kam.lam@fhusainc.com>
        
        Editor:   Xiang YUN
                  <mailto:yunxig@fiberhome.com>
        
        Editor:   Scott MANSFIELD
                  <scott.mansfield@ericsson.com>
        
        Editor:   Italo BUSI
                  <Italo.Busi@huawei.com>";
    description "This itut-eth-oam UML model (module) of ITU-T Recommendation G.8052.1 specifies the Transport Ethernet OAM information model, which is pruned and refactored from the base G.8052 UML model. 
        Version 2.00 (09/2022)";
    revision 2022-07-05 {
        description "Initial Version.";
        reference "ITU-T G.8052.1 v1.0.4 (2022/XX)";
    }
    augment "/dot1q-cfm:cfm/dot1q-cfm:maintenance-domain"{        uses eth-mip-spec;        description "none";    }    augment "/dot1q-cfm:cfm/dot1q-cfm:maintenance-group/dot1q-cfm:mep"{        uses eth-mep-oam-spec;        description "none";    }    /********************************************
    * grouping statements for object references
    ********************************************/ 
