            leaf alarm-severity-assignment-profile-asap-id {
                type leafref {
                    path '/dot1q-cfm:cfm/dot1q-cfm:maintenance-group/dot1q-cfm:mep/itut-eth-oam:asap/itut-eth-oam:asap-id';
                }
                description "none";
                reference "none";
            }
            description "none";
            reference "none";
        }
        grouping threshold-profile-ref {
            leaf threshold-profile-threshold-profile-id {
                type leafref {
                    path '../tp-dummy';
                }
                description "none";
                reference "none";
            }
            leaf tp-dummy {
               type string;
            }
            uses current-data-ref;
            description "none";
            reference "none";
        }
        grouping current-data-ref {
            leaf current-data-granularity-period {
                type leafref {
                    path '../cd-dummy';
                }
                description "none";
                reference "none";
            }
            leaf cd-dummy {
               type string;
            }
            description "none";
            reference "ITU-T Recommendation Q.822 1994-04: 4.3.1.1";
        }
        grouping dual-ended-current-data-ref {
            leaf dual-ended-current-data-granularity-period {
                type leafref {
                    path '/dot1q-cfm:cfm/dot1q-cfm:maintenance-group/dot1q-cfm:mep/itut-eth-oam:measurement-job/itut-eth-oam:pro-active-dual-ended/itut-eth-oam:target/itut-eth-oam:current-data/itut-eth-oam:granularity-period';
                }
                description "none";
                reference "none";
            }
            description "none";
            reference "none";
        }
        grouping dual-ended-history-data-ref {
            leaf dual-ended-history-data-period-end-time {
                type leafref {
                    path '/dot1q-cfm:cfm/dot1q-cfm:maintenance-group/dot1q-cfm:mep/itut-eth-oam:measurement-job/itut-eth-oam:pro-active-dual-ended/itut-eth-oam:target/itut-eth-oam:current-data/itut-eth-oam:history-data/itut-eth-oam:period-end-time';
                }
                description "none";
                reference "none";
            }
            uses dual-ended-current-data-ref;
            description "none";
            reference "none";
        }
        grouping single-ended-current-data-ref {
            leaf single-ended-current-data-granularity-period {
                type leafref {
                    path '/dot1q-cfm:cfm/dot1q-cfm:maintenance-group/dot1q-cfm:mep/itut-eth-oam:measurement-job/itut-eth-oam:pro-active-single-ended/itut-eth-oam:control/itut-eth-oam:current-data/itut-eth-oam:granularity-period';
                }
                description "none";
                reference "none";
            }
            description "none";
            reference "none";
        }
        grouping single-ended-history-data-ref {
            leaf single-ended-history-data-period-end-time {
                type leafref {
                    path '/dot1q-cfm:cfm/dot1q-cfm:maintenance-group/dot1q-cfm:mep/itut-eth-oam:measurement-job/itut-eth-oam:pro-active-single-ended/itut-eth-oam:control/itut-eth-oam:current-data/itut-eth-oam:history-data/itut-eth-oam:period-end-time';
                }
                description "none";
                reference "none";
            }
            uses single-ended-current-data-ref;
            description "none";
            reference "none";
        }
        grouping history-data-ref {
            leaf history-data-period-end-time {
                type leafref {
                    path '/itut-eth-oam:history-data/itut-eth-oam:period-end-time';
                }
                description "none";
                reference "none";
            }
            description "none";
            reference "ITU-T Q.822 v1.1 (03/2003): clause 4.3.1.12.";
        }
        grouping mip-ref {
            leaf mip-name {
                type leafref {
                    path '/dot1q-cfm:cfm/dot1q-cfm:maintenance-domain/itut-eth-oam:mip/itut-eth-oam:name';
                }
                description "none";
                reference "none";
            }
            description "none";
            reference "ITU-T G.8013 v5.3 (08/2019): clauses 3.2.9 and 6.";
        }
        grouping eth-mea-job-pac-ref {
            leaf eth-mea-job-pac-measurement-job-id {
                type leafref {
                    path '/dot1q-cfm:cfm/dot1q-cfm:maintenance-group/dot1q-cfm:mep/itut-eth-oam:measurement-job/itut-eth-oam:measurement-job-id';
                }
                description "none";
                reference "none";
            }
            description "none";
            reference "none";
        }
        grouping alarm-severity-assignment-ref {
            leaf alarm-severity-assignment-problem {
                type leafref {
                    path '/itut-eth-oam:alarm-severity-assignment/itut-eth-oam:problem';
                }
                description "none";
                reference "none";
            }
            uses alarm-severity-assignment-profile-ref;
            description "none";
            reference "none";
        }
        grouping one-dm-threshold-values-ref {
            leaf one-dm-threshold-values-granularity-period {
                type leafref {
                    path '/itut-eth-oam:one-dm-threshold-values/itut-eth-oam:granularity-period';
                }
                description "none";
                reference "none";
            }
            uses threshold-profile-ref;
            description "none";
            reference "none";
        }
        grouping one-lm-threshold-values-ref {
            leaf one-lm-threshold-values-granularity-period {
                type leafref {
                    path '/itut-eth-oam:one-lm-threshold-values/itut-eth-oam:granularity-period';
                }
                description "none";
                reference "none";
            }
            uses threshold-profile-ref;
            description "none";
            reference "none";
        }
        grouping dm-threshold-values-ref {
            leaf dm-threshold-values-granularity-period {
                type leafref {
                    path '/itut-eth-oam:dm-threshold-values/itut-eth-oam:granularity-period';
                }
                description "none";
                reference "none";
            }
            uses threshold-profile-ref;
            description "none";
            reference "none";
        }
        grouping lm-threshold-values-ref {
            leaf lm-threshold-values-granularity-period {
                type leafref {
                    path '/itut-eth-oam:lm-threshold-values/itut-eth-oam:granularity-period';
                }
                description "none";
                reference "none";
            }
            uses threshold-profile-ref;
            description "none";
            reference "none";
        }

    /****************************************
    * grouping statements for object classes
    **************************************/ 
        grouping alarm-severity-assignment-profile {
            leaf asap-id {
                type string;
                description "An identifier of the AlarmSeverityAssignmentProfile object.";
                reference "none";
            }
            list alarm-severity-assignment {
                key 'problem';
                uses alarm-severity-assignment;
                description "A list of all abnormal conditions that may exist in instances of an object class, 
                    and provides the assigned alarm severity information (minor, major, etc.) for each condition.";
                reference "none";
            }
            description "The alarm severity assignment profile object class is a class of management support object that specifies the alarm severity assignment for managed objects. 
                ";
            reference "none";
        }
        grouping threshold-profile {
            list dm-threshold-list {
                key 'granularity-period';
                uses dm-threshold-values;
                description "This attribute contains the threshold settings for the delay measurements (2-way) in the current registers. 
                    This attribute is defined as a list because it may contain lists for different granularity periods (e.g., 24hr, 15min, ...).";
                reference "none";
            }
            list one-dm-threshold-list {
                key 'granularity-period';
                uses one-dm-threshold-values;
                description "This attribute contains the threshold settings for the delay measurements (1-way) in the current registers. 
                    This attribute is defined as a list because it may contain lists for different granularity periods (e.g., 24hr, 15min, ...).";
                reference "none";
            }
            list lm-threshold-list {
                key 'granularity-period';
                uses lm-threshold-values;
                description "This attribute contains the threshold settings for the frame loss measurements (2-way) in the current registers. 
                    This attribute is defined as a list because it may contain lists for different granularity periods (e.g., 24hr, 15min, ...).";
                reference "none";
            }
            list one-lm-threshold-list {
                key 'granularity-period';
                uses one-lm-threshold-values;
                description "This attribute contains the threshold settings for the frame loss measurements (1-way) in the current registers. 
                    This attribute is defined as a list because it may contain lists for different granularity periods (e.g., 24hr, 15min, ...).";
                reference "none";
            }
            leaf threshold-profile-id {
                type string;
                config false;
                description "The identifier of the threshold profile object.";
                reference "none";
            }
            description "This object class defines the PM threshold settings in the network element.
                Multiple ThresholdProfile instances may be created to support, e.g., different grade of services.
                These profiles are associated to the Current Data object instances modelling the current registers via a pointer from the Current Data instances.
                Basic attribute: _associatedCurrentDataList
                1DM threshold related attribute: 1DmThresholdList
                1LM threshold related attribute: 1LmThresholdList
                DM threshold related attribute: dmThresholdList
                LM threshold related attribute: lmThresholdList";
            reference "none";
        }
        grouping current-data {
            list associated-threshold-profile {
                key 'threshold-profile-threshold-profile-id';
                uses threshold-profile-ref;
                description "This attribute points to the associated ThresholdProfile intances that are used by the CurrentData for thresholding.";
                reference "none";
            }
            leaf suspect-interval-flag {
                type boolean;
                config false;
                description "The suspect interval flag.";
                reference "ITU-T Q.822 v1.1 (03/2003): clause 4.3.3.14.";
            }
            container elapsed-time {
                config false;
                uses time-interval;
                description "The difference between the current time and the start time of this current data.";
                reference "ITU-T Q.822 v1.1 (03/2003): clause 4.3.3.2.";
            }
            leaf granularity-period {
                type granularity-period;
                description "The granularity period (e.g., 15min/24h) of the Current Data. ";
                reference "ITU-T X.739 v1.2 (06/1998): clause 8.1.1.2";
            }
            leaf start-time {
                type generalized-time;
                config false;
                description "The interval start time. 
                    The value is bound to the quarter of an hour in case of a 15 minute interval and bound to the hour in case of a 24 hour interval.";
                reference "none";
            }
            description "The common attributes used by the performance monitoring Current Data.";
            reference "ITU-T Recommendation Q.822 1994-04: 4.3.1.1";
        }
        grouping dual-ended-current-data {
            container dm-data {
                if-feature when-the-oamtype-is-1dm;
                config false;
                uses pro-active-dual-ended-dm-data;
                description "The current pro-active delay measurement PM data.";
                reference "none";
            }
            list history-data {
                key 'period-end-time';
                uses dual-ended-history-data;
                description "The list of PM history data of a current data collected by a pro-active dual-ended measurement job.";
                reference "none";
            }
            container lm-data {
                if-feature when-the-oamtype-is-1sl-or-lm_ccm;
                config false;
                uses pro-active-dual-ended-lm-data;
                description "The current pro-active loss measurement PM data.";
                reference "none";
            }
            uses current-data;
            description "The PM current data collected by a pro-active dual-ended measurement job.";
            reference "none";
        }
        grouping dual-ended-history-data {
            container dm-data {
                if-feature when-the-oamtype-is-1dm;
                config false;
                uses pro-active-dual-ended-dm-data;
                description "The history pro-active dual-ended delay measurement PM data.";
                reference "none";
            }
            container lm-data {
                if-feature when-the-oamtype-is-1sl-or-lm_ccm;
                config false;
                uses pro-active-dual-ended-lm-data;
                description "The history pro-active dual-ended loss measurement PM data.";
                reference "none";
            }
            uses history-data;
            description "The PM history data of a current data collected by a pro-active dual-ended measurement job.";
            reference "none";
        }
        grouping single-ended-current-data {
            container lm-data {
                config false;
                uses pro-active-single-ended-lm-data;
                description "This attribute contains the pro-active loss measurement PM data.";
                reference "none";
            }
            list history-data {
                key 'period-end-time';
                uses single-ended-history-data;
                description "The list of PM history data of a current data collected by a pro-active single-ended measurement job.";
                reference "none";
            }
            container dm-data {
                if-feature when-the-oamtype-is-dmm-dmr;
                config false;
                uses pro-active-single-ended-dm-data;
                description "The current pro-active delay measurement PM data.";
                reference "none";
            }
            uses current-data;
            description "The PM current data collected by a pro-active single-ended measurement job.";
            reference "none";
        }
        grouping single-ended-history-data {
            container lm-data {
                config false;
                uses pro-active-single-ended-lm-data;
                description "This attribute contains the near and far end pro-active PM data.";
                reference "none";
            }
            container dm-data {
                if-feature when-the-oamtype-is-dmm-dmr;
                config false;
                uses pro-active-single-ended-dm-data;
                description "The history pro-active single-ended delay measurement PM data.";
                reference "none";
            }
            uses history-data;
            description "The PM history data of a current data collected by a pro-active single-ended measurement job.";
            reference "none";
        }
        grouping history-data {
            leaf period-end-time {
                type generalized-time;
                description "This attribute contains the time at the end of the interval.";
                reference "ITU-T Q.822 v1.1 (03/2003): clause 4.3.3.12.";
            }
            leaf granularity-period {
                type granularity-period;
                config false;
                description "The granularity period (e.g., 15min/24h) of the History Data.";
                reference "ITU-T X.739 v1.2 (06/1998): clause 8.1.1.2.";
            }
            leaf suspect-interval-flag {
                type boolean;
                config false;
                description "The suspected interval flag.";
                reference "ITU-T Q.822 v1.1 (03/2003): clause 4.3.3.14.";
            }
            description "The common attributes used by the performance monitoring History Data.";
            reference "ITU-T Q.822 v1.1 (03/2003): clause 4.3.1.12.";
        }
        grouping on-demand-dual-ended-mea-job {
            container target {
                uses on-demand-dual-ended-mea-job-control-target;
                description "none";
                reference "none";
            }
            container initiator {
                uses on-demand-dual-ended-mea-job-control-initiator;
                description "none";
                reference "none";
            }
            description "none";
            reference "none";
        }
        grouping on-demand-single-ended-mea-job {
            container control {
                uses on-demand-single-ended-mea-job-control;
                description "none";
                reference "none";
            }
            description "none";
            reference "none";
        }
        grouping pro-active-dual-ended-mea-job {
            container target {
                uses pro-active-dual-ended-mea-job-control-target;
                description "A pro-active dual-ended measurement job control within a receiving MEP.";
                reference "none";
            }
            container initiator {
                uses pro-active-dual-ended-mea-job-control-initiator;
                description "A pro-active dual-ended measurement job control within an initiating MEP.";
                reference "none";
            }
            description "It contains the attributes of a pro-active dual-ended measurement job.
                ";
            reference "none";
        }
        grouping pro-active-single-ended-mea-job {
            container control {
                uses pro-active-single-ended-mea-job-control;
                description "A pro-active single-ended measurement job control within an initiating MEP.";
                reference "none";
            }
            description "It contains the attributes of a pro-active single-ended measurement job.";
            reference "none";
        }
        grouping on-demand-dual-ended-mea-job-control-target {
            leaf start-time {
                type generalized-time;
                config false;
                description "Start time of the on-demand measurement job.";
                reference "none";
            }
            leaf stop-time {
                type generalized-time;
                config false;
                description "Stop time of the on-demand measurement job.
                    If the stop time is not reached yet, the OAM PM process is running: the received OAM messages are processed, and the PM data are collected and reported. 
                    If the stop time is reached, the OAM PM process is stopped: the OAM PM process resources are still allocated but the received OAM messages are ignored, and the PM data collected at the time the OAM PM process is stopped are still reported.";
                reference "none";
            }
            leaf source-address {
                type mac-address;
                config false;
                description "This attribute contains the MAC address of the peer MEP. See G.8013 for details.";
                reference "none";
            }
            leaf oam-type {
                type dual-ended-oam-type;
                config false;
                description "This attribute contains the type of the 1-way measurement.";
                reference "none";
            }
            leaf test-identifier {
                if-feature mandatory-1sl-optional-1dm-;
                type int64 {
                    range "0..4294967295";
                }
                config false;
                description "This attribute is used to distinguish each measurement session if multiple measurement sessions are simultaneously activated towards a peer MEP including concurrent on-demand and proactive tests. It must be unique at least within the context of any measurement type for the MEG and initiating MEP.";
                reference "none";
            }
            container on-demand-performance-data {
                config false;
                uses on-demand-sink-performance-data;
                description "This attribute provides the final (at the end of the measurement interval) PM data report. The management system receives the final PM data report via the attributeValueChangeNotification of this attribute.";
                reference "none";
            }
            leaf priority {
                type string {
                }
                config false;
                description "The priority value of the OAM messages generated by the MEP for this measurement job. 
                    The default value is the same as the CCM priority.";
                reference "none";
            }
            description "It is created as a result of an establishOnDemandDualEndedMeasurementJobSink() operation. It is deleted either automatically after the measurement job has completed (stop time reached) and the performance data AVC notification has been sent, or by an explicit abortOnDemandMeasurementJob() operation when the measurement job is running.
                It contains the attributes of an on-demand dual-ended measurement job control within a receiving MEP.
                Basic attributes: oamType, priority, sourceAddress, startTime, stopTime, testIdentifier
                Performance data related attribute: onDemandPerformanceData
                Performance data related operation: getIntermediateReport";
            reference "none";
        }
        grouping on-demand-dual-ended-mea-job-control-initiator {
            leaf oam-type {
                type dual-ended-oam-type;
                config false;
                description "This attribute contains the type of the 1-way measurement.";
                reference "none";
            }
            container dm {
                uses oam-dm-config;
                description "none";
                reference "none";
            }
            container sl {
                uses oam-sl-config;
                description "none";
                reference "none";
            }
            uses on-demand-measurement-job-control;
            description "It contains the attributes of an on-demand dual-ended measurement job control within an initiating MEP.
                The description of the inherited stopTime attribute is redefined as:
                If the stop time is not reached yet, the OAM PM process is running: the OAM messages are generated. 
                If the stop time is reached, the OAM PM process is stopped: the OAM PM process resources are still allocated but OAM messages are not generated.";
            reference "none";
        }
        grouping on-demand-measurement-job-control {
            leaf start-time {
                type generalized-time;
                description "Start time of the on-demand measurement job.";
                reference "none";
            }
            leaf stop-time {
                type generalized-time;
                description "Stop time of the on-demand measurement job.";
                reference "none";
            }
            leaf destination-address {
                type mac-address;
                config false;
                description "This attribute contains the MAC address of the peer MEP. See G.8013 for details.";
                reference "none";
            }
            leaf oam-pdu-generation-type {
                type oam-pdu-generation-type;
                config false;
                description "This attribute contains the pattern that is used for the generation of OAM PDUs.";
                reference "none";
            }
            leaf measurement-interval {
                type int64;
                description "This attribute contains the discrete non overlapping periods of time (in seconds) during which measurements are performed (i.e., OAM messages are generated) and reports are gathered at the end of the measurement intervals. 
                    Note that the value 0 means a degenerated measurement interval with a single OAM message and the report is sent as immediately as possible.";
                reference "none";
            }
            leaf repetition-period {
                type repetition-period;
                description "This attribute contains the time between the start of two measurement intervals. This IS applicable for the repetitive instance type and MAY be applicable for the repetitive series type. 
                    Note that a value of 0 means not applicable (NA), which is for the cases of single instance, single series, or repetitive series without extra gap in between the measurement intervals (i.e., also as known as continuous series).";
                reference "none";
            }
            leaf priority {
                type string {
                }
                config false;
                description "The priority value of the OAM messages generated by the MEP for this measurement job. 
                    The default value is the same as the CCM priority.";
                reference "none";
            }
            description "This object class contains the common attributes representing the configuration of an on-demand measurement job.
                Basic attributes: destinationAddress, priority
                Measurement configuration related attributes: oamPduGenerationType, startTime, stopTime, messagePeriod, repetitionPeriod, measurementInterval
                Optional attributes: dataTlvLength, testIdentifier";
            reference "none";
        }
        grouping on-demand-single-ended-mea-job-control {
            leaf oam-type {
                type single-ended-oam-type;
                config false;
                description "This attribute contains the type of the 2-way measurement.";
                reference "none";
            }
            container on-demand-performance-data {
                config false;
                uses on-demand-performance-data;
                description "This attribute provides the final (at the end of the measurement interval) PM data report. The OS receives the final PM data report via the attributeValueChangeNotification of this attribute.";
                reference "none";
            }
            container dm {
                uses oam-dm-config;
                description "none";
                reference "none";
            }
            container sl {
                uses oam-sl-config;
                description "none";
                reference "none";
            }
            container lm {
                uses oam-lm-config;
                description "none";
                reference "none";
            }
            uses on-demand-measurement-job-control;
            description "It contains the attributes of an on-demand single-ended measurement job control within an initiating MEP.
                The description of the inherited stopTime attribute is redefined as:
                If the stop time is not reached yet, the OAM PM process is running: the OAM messages are generated; the received OAM messages are processed, and the PM data are collected and reported. 
                If the stop time is reached, the OAM PM process is stopped: the OAM PM process resources are still allocated but OAM messages are not generated; the received OAM messages are ignored, and the PM data collected at the time the measurement job has been stopped are still reported.";
            reference "none";
        }
        grouping pro-active-dual-ended-mea-job-control-target {
            leaf enabled {
                type boolean;
                default "true";
                description "Enables the pro-active measurement job.
                    If set to True, the OAM PM process is running: the received OAM messages are processed and the PM data are collected and reported. 
                    If set to False, the OAM PM process is stopped: the OAM PM process resources are still allocated but the received OAM messages are ignored and the PM data collected at the time the measurement job has been disabled are still reported; the current data suspect interval flag is set.  ";
                reference "none";
            }
            leaf source-address {
                if-feature oamtype-is-1dm-or-1sl;
                type string;
                description "This attribute contains the MAC address of the peer MEP. 
                    ";
                reference "ITU-T G.8013 v5.3 (08/2019): clause 10.";
            }
            leaf test-identifier {
                if-feature mandatory-1sl-optional-1dm-not-supported-lm_ccm;
