                description "The type of the OAM protocol.";
                reference "none";
            }
            description "The common attributes of a pro-active measurement job control.
                ";
            reference "none";
        }
        grouping pro-active-single-ended-mea-job-control {
            list current-data {
                key 'granularity-period';
