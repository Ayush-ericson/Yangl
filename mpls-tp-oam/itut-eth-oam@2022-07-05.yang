                reference "none";
            }
            container sl {
                uses oam-sl-config;
                description "Single-ended SLM/SLR OAM protocol configuration.";
                reference "none";
            }
            container lm {
                uses oam-lm-config;
                description "Single-ended LMM/LMR OAM protocol configuration.";
                reference "none";
            }
            uses pro-active-measurement-job-control;
            description "It contains the attributes of a pro-active single-ended measurement job control within an initiating MEP.
                The description of the enabled attribute is redefined as:
                If set to True, the OAM PM process is running: the OAM messages are generated; the received OAM messages are processed and the PM data are collected and reported. 
                If set to False, the OAM PM process is stopped: the OAM PM process resources are still allocated but OAM messages are not generated; the received OAM messages are ignored and the PM data collected at the time the measurement job has been disabled are still reported; the current data suspect interval flag is set.";
