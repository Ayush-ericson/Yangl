            container sl {
                uses oam-sl-config;
                description "Dual-ended 1SL OAM protocol configuration.";
                reference "none";
            }
            uses pro-active-measurement-job-control;
            description "It contains the attributes of a pro-active dual-ended measurement job control within an initiating MEP.
                The description of the enabled attribute is redefined as:
                If set to True, the OAM PM process is running: the OAM messages are generated. 
                If set to False, the OAM PM process is stopped: the OAM PM process resources are still allocated but OAM messages are not generated.";
            reference "none";
        }
        grouping pro-active-measurement-job-control {
            leaf enabled {
                type boolean;
                default "true";
                description "Enables the pro-active measurement job.";
                reference "none";
            }
            leaf destination-address {
                if-feature when-the-oamtype-is-not-ccm;
                type mac-address;
