          "connection-group": [
            {
              "id": "matrix1",
              "connectivity-type": "ietf-vpn-common:any-to-any",
              "connectivity-construct": [
                {
                  "id": "1",
                  "a2a-sdp": [
                    {
                      "sdp-id": "1"
