                    "ac-ipv4-address": "192.0.2.1",
                    "ac-ipv4-prefix-length": 26,
                    "ac-tags": {
                      "ac-tag": [
                        {
                          "tag-type": "ietf-network-slice-service:vlan-id",
                          "value": [
                            "100"
