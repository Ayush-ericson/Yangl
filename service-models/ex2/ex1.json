                "match-criterion": [
                  {
                    "index": 1,
                    "match-type": "ietf-network-slice-service:any",
                    "target-connection-group-id": "matrix1",
                    "target-connectivity-construct-id": "1"
                  }
                ]
              },
              "attachment-circuits": {
                "attachment-circuit": [
                  {
                    "id": "ac2",
                    "description": "AC2 connected to device 2",
                    "ac-node-id": "PE-B",
