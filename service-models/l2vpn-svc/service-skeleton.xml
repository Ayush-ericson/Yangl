<?xml version='1.0' encoding='UTF-8'?>
<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <l2vpn-svc xmlns="urn:ietf:params:xml:ns:yang:ietf-l2vpn-svc">
    <vpn-profiles>
      <valid-provider-identifiers>
        <cloud-identifier>
          <!-- # entries: 0.. -->
        </cloud-identifier>
        <qos-profile-identifier>
          <!-- # entries: 0.. -->
        </qos-profile-identifier>
        <bfd-profile-identifier>
          <!-- # entries: 0.. -->
        </bfd-profile-identifier>
        <remote-carrier-identifier>
          <!-- # entries: 0.. -->
        </remote-carrier-identifier>
      </valid-provider-identifiers>
    </vpn-profiles>
    <vpn-services>
      <vpn-service>
        <vpn-id/>
        <vpn-svc-type>vpws</vpn-svc-type>
        <customer-name/>
        <svc-topo>any-to-any</svc-topo>
        <cloud-accesses>
          <cloud-access>
            <cloud-identifier/>
            <permit-any/>
            <permit-site>
              <!-- # entries: 0.. -->
            </permit-site>
            <deny-site>
              <!-- # entries: 0.. -->
            </deny-site>
          </cloud-access>
        </cloud-accesses>
        <frame-delivery>
          <customer-tree-flavors>
            <tree-flavor>
              <!-- # entries: 0.. -->
            </tree-flavor>
          </customer-tree-flavors>
          <bum-deliveries>
            <bum-delivery>
              <frame-type/>
              <delivery-mode>unconditional</delivery-mode>
            </bum-delivery>
          </bum-deliveries>
          <multicast-gp-port-mapping/>
        </frame-delivery>
        <extranet-vpns>
          <extranet-vpn>
            <vpn-id/>
            <local-sites-role>any-to-any-role</local-sites-role>
          </extranet-vpn>
        </extranet-vpns>
        <ce-vlan-preservation/>
        <ce-vlan-cos-preservation/>
        <carrierscarrier>false</carrierscarrier>
      </vpn-service>
    </vpn-services>
    <sites>
      <site>
        <site-id/>
        <site-vpn-flavor>site-vpn-flavor-single</site-vpn-flavor>
        <devices>
          <device>
            <device-id/>
            <location/>
            <management>
              <transport/>
              <address/>
            </management>
          </device>
        </devices>
        <management>
          <type/>
        </management>
        <locations>
          <location>
            <location-id/>
            <address/>
            <postal-code/>
            <state/>
            <city/>
            <country-code/>
          </location>
        </locations>
        <site-diversity>
          <groups>
            <group>
              <group-id/>
            </group>
          </groups>
        </site-diversity>
        <vpn-policies>
          <vpn-policy>
            <vpn-policy-id/>
            <entries>
              <id/>
              <filters>
                <filter>
                  <type/>
                  <lan-tag>
                    <!-- # entries: 0.. -->
                  </lan-tag>
                </filter>
              </filters>
              <vpn>
                <vpn-id/>
                <site-role>any-to-any-role</site-role>
              </vpn>
            </entries>
          </vpn-policy>
        </vpn-policies>
        <service>
          <qos>
            <qos-classification-policy>
              <rule>
                <id/>
                <match-flow>
                  <dscp/>
                  <dot1q/>
                  <pcp/>
                  <src-mac/>
                  <dst-mac/>
                  <color-type/>
                  <target-sites>
                    <!-- # entries: 0.. -->
                  </target-sites>
                  <any/>
                  <vpn-id/>
                </match-flow>
                <match-application/>
                <target-class-id/>
              </rule>
            </qos-classification-policy>
            <qos-profile>
              <profile/>
              <classes>
                <class>
                  <class-id/>
                  <direction>bidirectional</direction>
                  <policing>one-rate-two-color</policing>
                  <byte-offset/>
                  <frame-delay>
                    <use-lowest-latency/>
                    <delay-bound/>
                  </frame-delay>
                  <frame-jitter>
                    <use-lowest-jitter/>
                    <delay-bound/>
                  </frame-jitter>
                  <frame-loss>
                    <rate/>
                  </frame-loss>
                  <bandwidth>
                    <guaranteed-bw-percent/>
                    <end-to-end/>
                  </bandwidth>
                </class>
              </classes>
            </qos-profile>
          </qos>
          <carrierscarrier>
            <signaling-type>bgp</signaling-type>
          </carrierscarrier>
        </service>
        <broadcast-unknown-unicast-multicast>
          <multicast-site-type>source-receiver</multicast-site-type>
          <multicast-gp-address-mapping>
            <id/>
            <vlan-id/>
            <mac-gp-address/>
            <port-lag-number/>
          </multicast-gp-address-mapping>
          <bum-overall-rate/>
          <bum-rate-per-type>
            <type/>
            <rate/>
          </bum-rate-per-type>
        </broadcast-unknown-unicast-multicast>
        <mac-loop-prevention>
          <protection-type>trap</protection-type>
          <frequency>5</frequency>
          <retry-timer/>
        </mac-loop-prevention>
        <access-control-list>
          <mac>
            <mac-address/>
          </mac>
        </access-control-list>
        <actual-site-start/>
        <actual-site-stop/>
        <bundling-type>one2one-bundling</bundling-type>
        <default-ce-vlan-id/>
        <site-network-accesses>
          <site-network-access>
            <network-access-id/>
            <remote-carrier-name/>
            <type>point-to-point</type>
            <location-reference/>
            <device-reference/>
            <access-diversity>
              <groups>
                <group>
                  <group-id/>
                </group>
              </groups>
              <constraints>
                <constraint>
                  <constraint-type/>
                  <target>
                    <group>
                      <group-id/>
                    </group>
                    <all-other-accesses/>
                    <all-other-groups/>
                  </target>
                </constraint>
              </constraints>
            </access-diversity>
            <bearer>
              <requested-type>
                <type/>
                <strict>false</strict>
              </requested-type>
              <always-on>true</always-on>
              <bearer-reference/>
            </bearer>
            <connection>
              <encapsulation-type>ethernet</encapsulation-type>
              <eth-inf-type>untagged</eth-inf-type>
              <tagged-interface>
                <type>priority-tagged</type>
                <dot1q-vlan-tagged>
                  <tg-type>c-vlan</tg-type>
                  <cvlan-id/>
                </dot1q-vlan-tagged>
                <priority-tagged>
                  <tag-type>c-vlan</tag-type>
                </priority-tagged>
                <qinq>
                  <tag-type>c-s-vlan</tag-type>
                  <svlan-id/>
                  <cvlan-id/>
                </qinq>
                <qinany>
                  <tag-type>s-vlan</tag-type>
                  <svlan-id/>
                </qinany>
                <vxlan>
                  <vni-id/>
                  <peer-mode>static-mode</peer-mode>
                  <peer-list>
                    <peer-ip/>
                  </peer-list>
                </vxlan>
              </tagged-interface>
              <untagged-interface>
                <speed>10</speed>
                <mode>auto-neg</mode>
                <phy-mtu/>
                <lldp>false</lldp>
                <oam-802.3ah-link>
                  <enabled>false</enabled>
                </oam-802.3ah-link>
                <uni-loop-prevention>false</uni-loop-prevention>
              </untagged-interface>
              <lag-interfaces>
                <lag-interface>
                  <index/>
                  <lacp>
                    <enabled>false</enabled>
                    <mode/>
                    <speed>10</speed>
                    <mini-link-num/>
                    <system-priority>32768</system-priority>
                    <micro-bfd>
                      <enabled>off</enabled>
                      <interval/>
                      <hold-timer/>
                    </micro-bfd>
                    <bfd>
                      <enabled>false</enabled>
                      <profile-name/>
                      <fixed-value/>
                    </bfd>
                    <member-links>
                      <member-link>
                        <name/>
                        <speed>10</speed>
                        <mode>auto-neg</mode>
                        <link-mtu/>
                        <oam-802.3ah-link>
                          <enabled>false</enabled>
                        </oam-802.3ah-link>
                      </member-link>
                    </member-links>
                    <flow-control>false</flow-control>
                    <lldp>false</lldp>
                  </lacp>
                </lag-interface>
              </lag-interfaces>
              <cvlan-id-to-svc-map>
                <svc-id/>
                <cvlan-id>
                  <vid/>
                </cvlan-id>
              </cvlan-id-to-svc-map>
              <l2cp-control>
                <stp-rstp-mstp/>
                <pause/>
                <lacp-lamp/>
                <link-oam/>
                <esmc/>
                <l2cp-802.1x/>
                <e-lmi/>
                <lldp/>
                <ptp-peer-delay/>
                <garp-mrp/>
              </l2cp-control>
              <oam>
                <md-name/>
                <md-level/>
                <cfm-8021-ag>
                  <maid/>
                  <mep-id/>
                  <mep-level/>
                  <mep-up-down>up</mep-up-down>
                  <remote-mep-id/>
                  <cos-for-cfm-pdus/>
                  <ccm-interval>10000</ccm-interval>
                  <ccm-holdtime>35000</ccm-holdtime>
                  <alarm-priority-defect>remote-invalid-ccm</alarm-priority-defect>
                  <ccm-p-bits-pri/>
                </cfm-8021-ag>
                <y-1731>
                  <maid/>
                  <mep-id/>
                  <type>delay</type>
                  <remote-mep-id/>
                  <message-period>10000</message-period>
                  <measurement-interval/>
                  <cos/>
                  <loss-measurement>false</loss-measurement>
                  <synthetic-loss-measurement>false</synthetic-loss-measurement>
                  <delay-measurement>
                    <enable-dm>false</enable-dm>
                    <two-way>false</two-way>
                  </delay-measurement>
                  <frame-size/>
                  <session-type>on-demand</session-type>
                </y-1731>
              </oam>
            </connection>
            <availability>
              <access-priority>100</access-priority>
              <single-active/>
              <all-active/>
            </availability>
            <vpn-attachment>
              <vpn-id/>
              <site-role>any-to-any-role</site-role>
              <vpn-policy-id/>
            </vpn-attachment>
            <service>
              <svc-bandwidth>
                <bandwidth>
                  <direction/>
                  <type/>
                  <cos-id/>
                  <vpn-id/>
                  <cir/>
                  <cbs/>
                  <eir/>
                  <ebs/>
                  <pir/>
                  <pbs/>
                </bandwidth>
              </svc-bandwidth>
              <svc-mtu/>
              <qos>
                <qos-classification-policy>
                  <rule>
                    <id/>
                    <match-flow>
                      <dscp/>
                      <dot1q/>
                      <pcp/>
                      <src-mac/>
                      <dst-mac/>
                      <color-type/>
                      <target-sites>
                        <!-- # entries: 0.. -->
                      </target-sites>
                      <any/>
                      <vpn-id/>
                    </match-flow>
                    <match-application/>
                    <target-class-id/>
                  </rule>
                </qos-classification-policy>
                <qos-profile>
                  <profile/>
                  <classes>
                    <class>
                      <class-id/>
                      <direction>bidirectional</direction>
                      <policing>one-rate-two-color</policing>
                      <byte-offset/>
                      <frame-delay>
                        <use-lowest-latency/>
                        <delay-bound/>
                      </frame-delay>
                      <frame-jitter>
                        <use-lowest-jitter/>
                        <delay-bound/>
                      </frame-jitter>
                      <frame-loss>
                        <rate/>
                      </frame-loss>
                      <bandwidth>
                        <guaranteed-bw-percent/>
                        <end-to-end/>
                      </bandwidth>
                    </class>
                  </classes>
                </qos-profile>
              </qos>
              <carrierscarrier>
                <signaling-type>bgp</signaling-type>
              </carrierscarrier>
            </service>
            <broadcast-unknown-unicast-multicast>
              <multicast-site-type>source-receiver</multicast-site-type>
              <multicast-gp-address-mapping>
                <id/>
                <vlan-id/>
                <mac-gp-address/>
                <port-lag-number/>
              </multicast-gp-address-mapping>
              <bum-overall-rate/>
              <bum-rate-per-type>
                <type/>
                <rate/>
              </bum-rate-per-type>
            </broadcast-unknown-unicast-multicast>
            <mac-loop-prevention>
              <protection-type>trap</protection-type>
              <frequency>5</frequency>
              <retry-timer/>
            </mac-loop-prevention>
            <access-control-list>
              <mac>
                <mac-address/>
              </mac>
            </access-control-list>
            <mac-addr-limit>
              <limit-number>2</limit-number>
              <time-interval>300</time-interval>
              <action>warning</action>
            </mac-addr-limit>
          </site-network-access>
        </site-network-accesses>
      </site>
    </sites>
  </l2vpn-svc>
</data>
