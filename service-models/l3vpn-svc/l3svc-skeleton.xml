<?xml version='1.0' encoding='UTF-8'?>
<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <l3vpn-svc xmlns="urn:ietf:params:xml:ns:yang:ietf-l3vpn-svc">
    <vpn-profiles>
      <valid-provider-identifiers>
        <cloud-identifier>
          <id/>
        </cloud-identifier>
        <encryption-profile-identifier>
          <id/>
        </encryption-profile-identifier>
        <qos-profile-identifier>
          <id/>
        </qos-profile-identifier>
        <bfd-profile-identifier>
          <id/>
        </bfd-profile-identifier>
      </valid-provider-identifiers>
    </vpn-profiles>
    <vpn-services>
      <vpn-service>
        <vpn-id/>
        <customer-name/>
        <vpn-service-topology>any-to-any</vpn-service-topology>
        <cloud-accesses>
          <cloud-access>
            <cloud-identifier/>
            <permit-any/>
            <permit-site>
              <!-- # entries: 0.. -->
            </permit-site>
            <deny-site>
              <!-- # entries: 0.. -->
            </deny-site>
            <address-translation>
              <nat44>
                <enabled>false</enabled>
                <nat44-customer-address/>
              </nat44>
            </address-translation>
          </cloud-access>
        </cloud-accesses>
        <multicast>
          <enabled>false</enabled>
          <customer-tree-flavors>
            <tree-flavor>
              <!-- # entries: 0.. -->
            </tree-flavor>
          </customer-tree-flavors>
          <rp>
            <rp-group-mappings>
              <rp-group-mapping>
                <id/>
                <provider-managed>
                  <enabled>false</enabled>
                  <rp-redundancy>false</rp-redundancy>
                  <optimal-traffic-delivery>false</optimal-traffic-delivery>
                </provider-managed>
                <rp-address/>
                <groups>
                  <group>
                    <id/>
                    <group-address/>
                    <group-start/>
                    <group-end/>
                  </group>
                </groups>
              </rp-group-mapping>
            </rp-group-mappings>
            <rp-discovery>
              <rp-discovery-type>static-rp</rp-discovery-type>
              <bsr-candidates>
                <bsr-candidate-address>
                  <!-- # entries: 0.. -->
                </bsr-candidate-address>
              </bsr-candidates>
            </rp-discovery>
          </rp>
        </multicast>
        <carrierscarrier>false</carrierscarrier>
        <extranet-vpns>
          <extranet-vpn>
            <vpn-id/>
            <local-sites-role>any-to-any-role</local-sites-role>
          </extranet-vpn>
        </extranet-vpns>
      </vpn-service>
    </vpn-services>
    <sites>
      <site>
        <site-id/>
        <requested-site-start/>
        <requested-site-stop/>
        <locations>
          <location>
            <location-id/>
            <address/>
            <postal-code/>
            <state/>
            <city/>
            <country-code/>
          </location>
        </locations>
        <devices>
          <device>
            <device-id/>
            <location/>
            <management>
              <address-family/>
              <address/>
            </management>
          </device>
        </devices>
        <site-diversity>
          <groups>
            <group>
              <group-id/>
            </group>
          </groups>
        </site-diversity>
        <management>
          <type/>
        </management>
        <vpn-policies>
          <vpn-policy>
            <vpn-policy-id/>
            <entries>
              <id/>
              <filters>
                <filter>
                  <type/>
                  <lan-tag>
                    <!-- # entries: 0.. -->
                  </lan-tag>
                  <ipv4-lan-prefix>
                    <!-- # entries: 0.. -->
                  </ipv4-lan-prefix>
                  <ipv6-lan-prefix>
                    <!-- # entries: 0.. -->
                  </ipv6-lan-prefix>
                </filter>
              </filters>
              <vpn>
                <vpn-id/>
                <site-role>any-to-any-role</site-role>
              </vpn>
            </entries>
          </vpn-policy>
        </vpn-policies>
        <site-vpn-flavor>site-vpn-flavor-single</site-vpn-flavor>
        <maximum-routes>
          <address-family>
            <af/>
            <maximum-routes/>
          </address-family>
        </maximum-routes>
        <security>
          <authentication/>
          <encryption>
            <enabled>false</enabled>
            <layer/>
            <encryption-profile>
              <profile-name/>
              <algorithm/>
              <preshared-key/>
            </encryption-profile>
          </encryption>
        </security>
        <service>
          <qos>
            <qos-classification-policy>
              <rule>
                <id/>
                <match-flow>
                  <dscp/>
                  <dot1p/>
                  <ipv4-src-prefix/>
                  <ipv6-src-prefix/>
                  <ipv4-dst-prefix/>
                  <ipv6-dst-prefix/>
                  <l4-src-port/>
                  <target-sites>
                    <!-- # entries: 0.. -->
                  </target-sites>
                  <l4-src-port-range>
                    <lower-port/>
                    <upper-port/>
                  </l4-src-port-range>
                  <l4-dst-port/>
                  <l4-dst-port-range>
                    <lower-port/>
                    <upper-port/>
                  </l4-dst-port-range>
                  <protocol-field/>
                </match-flow>
                <match-application/>
                <target-class-id/>
              </rule>
            </qos-classification-policy>
            <qos-profile>
              <profile/>
              <classes>
                <class>
                  <class-id/>
                  <direction>both</direction>
                  <rate-limit/>
                  <latency>
                    <use-lowest-latency/>
                    <latency-boundary>400</latency-boundary>
                  </latency>
                  <jitter>
                    <use-lowest-jitter/>
                    <latency-boundary>40000</latency-boundary>
                  </jitter>
                  <bandwidth>
                    <guaranteed-bw-percent/>
                    <end-to-end/>
                  </bandwidth>
                </class>
              </classes>
            </qos-profile>
          </qos>
          <carrierscarrier>
            <signalling-type>bgp</signalling-type>
          </carrierscarrier>
          <multicast>
            <multicast-site-type>source-receiver</multicast-site-type>
            <multicast-address-family>
              <ipv4>false</ipv4>
              <ipv6>false</ipv6>
            </multicast-address-family>
            <protocol-type>both</protocol-type>
          </multicast>
        </service>
        <traffic-protection>
          <enabled>false</enabled>
        </traffic-protection>
        <routing-protocols>
          <routing-protocol>
            <type/>
            <ospf>
              <address-family>
                <!-- # entries: 1.. -->
              </address-family>
              <area-address/>
              <metric>1</metric>
              <sham-links>
                <sham-link>
                  <target-site/>
                  <metric>1</metric>
                </sham-link>
              </sham-links>
            </ospf>
            <bgp>
              <autonomous-system/>
              <address-family>
                <!-- # entries: 1.. -->
              </address-family>
            </bgp>
            <static>
              <cascaded-lan-prefixes>
                <ipv4-lan-prefixes>
                  <lan/>
                  <next-hop/>
                  <lan-tag/>
                </ipv4-lan-prefixes>
                <ipv6-lan-prefixes>
                  <lan/>
                  <next-hop/>
                  <lan-tag/>
                </ipv6-lan-prefixes>
              </cascaded-lan-prefixes>
            </static>
            <rip>
              <address-family>
                <!-- # entries: 1.. -->
              </address-family>
            </rip>
            <vrrp>
              <address-family>
                <!-- # entries: 1.. -->
              </address-family>
            </vrrp>
          </routing-protocol>
        </routing-protocols>
        <actual-site-start/>
        <actual-site-stop/>
        <site-network-accesses>
          <site-network-access>
            <site-network-access-id/>
            <site-network-access-type>point-to-point</site-network-access-type>
            <location-reference/>
            <device-reference/>
            <access-diversity>
              <groups>
                <group>
                  <group-id/>
                </group>
              </groups>
              <constraints>
                <constraint>
                  <constraint-type/>
                  <target>
                    <group>
                      <group-id/>
                    </group>
                    <all-other-accesses/>
                    <all-other-groups/>
                  </target>
                </constraint>
              </constraints>
            </access-diversity>
            <bearer>
              <requested-type>
                <requested-type/>
                <strict>false</strict>
              </requested-type>
              <always-on>true</always-on>
              <bearer-reference/>
            </bearer>
            <ip-connection>
              <ipv4>
                <address-allocation-type/>
                <provider-dhcp>
                  <provider-address/>
                  <prefix-length/>
                  <number-of-dynamic-address>1</number-of-dynamic-address>
                  <customer-addresses>
                    <address-group>
                      <group-id/>
                      <start-address/>
                      <end-address/>
                    </address-group>
                  </customer-addresses>
                </provider-dhcp>
                <dhcp-relay>
                  <provider-address/>
                  <prefix-length/>
                  <customer-dhcp-servers>
                    <server-ip-address>
                      <!-- # entries: 0.. -->
                    </server-ip-address>
                  </customer-dhcp-servers>
                </dhcp-relay>
                <addresses>
                  <provider-address/>
                  <customer-address/>
                  <prefix-length/>
                </addresses>
              </ipv4>
              <ipv6>
                <address-allocation-type/>
                <provider-dhcp>
                  <provider-address/>
                  <prefix-length/>
                  <number-of-dynamic-address>1</number-of-dynamic-address>
                  <customer-addresses>
                    <address-group>
                      <group-id/>
                      <start-address/>
                      <end-address/>
                    </address-group>
                  </customer-addresses>
                </provider-dhcp>
                <dhcp-relay>
                  <provider-address/>
                  <prefix-length/>
                  <customer-dhcp-servers>
                    <server-ip-address>
                      <!-- # entries: 0.. -->
                    </server-ip-address>
                  </customer-dhcp-servers>
                </dhcp-relay>
                <addresses>
                  <provider-address/>
                  <customer-address/>
                  <prefix-length/>
                </addresses>
              </ipv6>
              <oam>
                <bfd>
                  <enabled>false</enabled>
                  <fixed-value/>
                  <profile-name/>
                </bfd>
              </oam>
            </ip-connection>
            <security>
              <authentication/>
              <encryption>
                <enabled>false</enabled>
                <layer/>
                <encryption-profile>
                  <profile-name/>
                  <algorithm/>
                  <preshared-key/>
                </encryption-profile>
              </encryption>
            </security>
            <service>
              <svc-input-bandwidth/>
              <svc-output-bandwidth/>
              <svc-mtu/>
              <qos>
                <qos-classification-policy>
                  <rule>
                    <id/>
                    <match-flow>
                      <dscp/>
                      <dot1p/>
                      <ipv4-src-prefix/>
                      <ipv6-src-prefix/>
                      <ipv4-dst-prefix/>
                      <ipv6-dst-prefix/>
                      <l4-src-port/>
                      <target-sites>
                        <!-- # entries: 0.. -->
                      </target-sites>
                      <l4-src-port-range>
                        <lower-port/>
                        <upper-port/>
                      </l4-src-port-range>
                      <l4-dst-port/>
                      <l4-dst-port-range>
                        <lower-port/>
                        <upper-port/>
                      </l4-dst-port-range>
                      <protocol-field/>
                    </match-flow>
                    <match-application/>
                    <target-class-id/>
                  </rule>
                </qos-classification-policy>
                <qos-profile>
                  <profile/>
                  <classes>
                    <class>
                      <class-id/>
                      <direction>both</direction>
                      <rate-limit/>
                      <latency>
                        <use-lowest-latency/>
                        <latency-boundary>400</latency-boundary>
                      </latency>
                      <jitter>
                        <use-lowest-jitter/>
                        <latency-boundary>40000</latency-boundary>
                      </jitter>
                      <bandwidth>
                        <guaranteed-bw-percent/>
                        <end-to-end/>
                      </bandwidth>
                    </class>
                  </classes>
                </qos-profile>
              </qos>
              <carrierscarrier>
                <signalling-type>bgp</signalling-type>
              </carrierscarrier>
              <multicast>
                <multicast-site-type>source-receiver</multicast-site-type>
                <multicast-address-family>
                  <ipv4>false</ipv4>
                  <ipv6>false</ipv6>
                </multicast-address-family>
                <protocol-type>both</protocol-type>
              </multicast>
            </service>
            <routing-protocols>
              <routing-protocol>
                <type/>
                <ospf>
                  <address-family>
                    <!-- # entries: 1.. -->
                  </address-family>
                  <area-address/>
                  <metric>1</metric>
                  <sham-links>
                    <sham-link>
                      <target-site/>
                      <metric>1</metric>
                    </sham-link>
                  </sham-links>
                </ospf>
                <bgp>
                  <autonomous-system/>
                  <address-family>
                    <!-- # entries: 1.. -->
                  </address-family>
                </bgp>
                <static>
                  <cascaded-lan-prefixes>
                    <ipv4-lan-prefixes>
                      <lan/>
                      <next-hop/>
                      <lan-tag/>
                    </ipv4-lan-prefixes>
                    <ipv6-lan-prefixes>
                      <lan/>
                      <next-hop/>
                      <lan-tag/>
                    </ipv6-lan-prefixes>
                  </cascaded-lan-prefixes>
                </static>
                <rip>
                  <address-family>
                    <!-- # entries: 1.. -->
                  </address-family>
                </rip>
                <vrrp>
                  <address-family>
                    <!-- # entries: 1.. -->
                  </address-family>
                </vrrp>
              </routing-protocol>
            </routing-protocols>
            <availability>
              <access-priority>100</access-priority>
            </availability>
            <vpn-attachment>
              <vpn-policy-id/>
              <vpn-id/>
              <site-role>any-to-any-role</site-role>
            </vpn-attachment>
          </site-network-access>
        </site-network-accesses>
      </site>
    </sites>
  </l3vpn-svc>
</data>
